{
  "name": "testcafe-reporter-reportportal-plugin-sap",
  "version": "0.2.0",
  "description": "Report Portal TestCafe reporter",
  "repository": "https://github.com/sahilshukla2603/reportportal-testcafe-plugin",
  "author": {
    "name": "Sahil Shukla",
    "url": "https://github.com/sahilshukla2603/"
  },
  "main": "lib/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "test": "mocha -r ts-node/register",
    "lint": "eslint .",
    "jshint": "jshint src",
    "unlink-pkg": "npm unlink --no-save testcafe-reporter-reportportal-plugin-sap && npm unlink && npm i",
    "link-pkg": "npm link && npm link testcafe-reporter-reportportal-plugin-sap",
    "build-local-reporter": "npm run unlink-pkg && npm run build && npm run link-pkg",
    "testcafe-test": "testcafe chrome:headless ./tests/test.testcafe.ts --reporter reportportal-plugin-sap",
    "unit-tests": "npm run test ./tests/unit/unit.executor.ts -- --rprotocol=http --rdomain=localhost:1234 --rlaunch-id=113 --rtoken=fff --rproject=tmp --rdescription=my_description --rattributes=environment:test|type:unit -g \"Running TestCafe Tests\"",
    "unit-retry-tests": "npm run test ./tests/unit/unit.executor.ts -- --rprotocol=http --rdomain=localhost:1234 --rlaunch-id=113 --rtoken=fff --rproject=retry --rdescription=my_description --rattributes=environment:test|type:retry -g \"Retry mechanism\"",
    "unit-no-live-reporting-tests": "npm run test ./tests/unit/unit.executor.ts -- --rprotocol=http --rdomain=localhost:1234 --rlaunch-id=113 --rtoken=fff --rproject=tmp --rdescription=my_description --rattributes=environment:test|type:no-live --disable-live-reporting -g \"Running TestCafe Tests\"",
    "unit-display-debug-logs-tests": "npm run test ./tests/unit/unit.executor.ts -- --rprotocol=http --rdomain=localhost:1234 --rlaunch-id=113 --rtoken=fff --rproject=tmp --rdescription=my_description --rattributes=environment:test|type:debug --display-debug-logs -g \"Running TestCafe Tests\"",
    "unit-attributes-tests": "npm run test ./tests/unit/unit.executor.ts -- --rprotocol=http --rdomain=localhost:1234 --rlaunch-id=113 --rtoken=fff --rproject=tmp --rdescription=my_description --rattributes=environment:test|type:unit -g \"Attributes functionality\"",
    "unit-suite-tests": "npm run test ./tests/unit/unit.executor.ts -- --rprotocol=http --rdomain=localhost:1234 --rlaunch-id=113 --rtoken=fff --rproject=tmp --rdescription=my_description --rsuite=TestSuite --rattributes=environment:test|type:suite -g \"Suite functionality\"",
    "unit-class-tests": "npm run test ./tests/unit/unit.report-portal.spec.ts",
    "run-unit-tests": "npm run build-local-reporter && npm run unit-tests",
    "run-unit-retry-tests": "npm run build-local-reporter && npm run unit-retry-tests",
    "run-unit-no-live-reporting-tests": "npm run build-local-reporter && npm run unit-no-live-reporting-tests",
    "run-unit-display-debug-logs-tests": "npm run build-local-reporter && npm run unit-display-debug-logs-tests",
    "run-unit-attributes-tests": "npm run build-local-reporter && npm run unit-attributes-tests",
    "run-unit-suite-tests": "npm run build-local-reporter && npm run unit-suite-tests",
    "run-unit-class-tests": "npm run build && npm run unit-class-tests",
    "run-full-unit-tests": "npm run build-local-reporter && npm run unit-tests && npm run unit-retry-tests && npm run unit-no-live-reporting-tests && npm run unit-display-debug-logs-tests && npm run unit-attributes-tests && npm run unit-suite-tests && npm run unit-class-tests",
    "integration-tests": "npm run test ./tests/integration/integration.executor.ts -- --rattributes=environment:integration|type:e2e --display-debug-logs",
    "run-integration-tests": "npm run build-local-reporter && npm run integration-tests",
    "download-report-portal-docker-compose-file": "curl https://raw.githubusercontent.com/reportportal/reportportal/a9f2fe18dfe9cb2a631d8a60fdbe254ced6963e7/docker-compose.yml -o docker-compose.yml",
    "download-report-portal-latest-docker-compose-file": "curl https://raw.githubusercontent.com/reportportal/reportportal/master/docker-compose.yml -o docker-compose.yml",
    "up": "docker compose -p reportportal up",
    "start": "docker compose -p reportportal start",
    "stop": "docker compose -p reportportal stop",
    "down": "docker compose -p reportportal down",
    "status": "docker compose -p reportportal ps -a"
  },
  "keywords": [
    "testcafe",
    "reporter",
    "plugin"
  ],
  "license": "Apache 2",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "callsite-record": "^3.2.0",
    "chai": "^4.3.3",
    "del": "^1.2.0",
    "dmock-server": "^1.5.5",
    "eslint": "8.5.0",
    "jshint": "^2.13.4",
    "mocha": "^9.2.0",
    "normalize-newline": "^1.0.2",
    "read-file-relative": "^1.2.0",
    "testcafe": "2.3.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "cli-argument-parser": "^0.5.9",
    "fs": "0.0.1-security"
  }
}
